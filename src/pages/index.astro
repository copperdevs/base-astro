---
import { BaseAppLayout } from "@app/layout/BaseAppLayout";
import BaseLayout from "@/layouts/BaseLayout.astro";
import {
  AccentColors,
  AppearanceColors,
  GrayColors,
  type AppInfo,
  type PageInfo,
  type PageProps,
} from "@/lib";

const props = {
  pageInfo: {
    title: "home",
    description: "home page for the base astro example",
  } as PageInfo,
  appInfo: {
    title: "base-astro",
    accentColor: AccentColors.Jade,
    grayColor: GrayColors.Slate,
    appearanceColor: AppearanceColors.Dark,
  } as AppInfo,
} as PageProps;
---

<BaseLayout props={props}>
  <BaseAppLayout props={props}>
    <> </>
  </BaseAppLayout>
</BaseLayout>
